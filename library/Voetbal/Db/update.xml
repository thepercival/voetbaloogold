	<Additions>
        <Version>V1.07</Version>
        <Addition>
            <Name>QueryExecute</Name>
            <queries>
                <sql>insert into PoulePlaceQualifyRules( FromPoulePlaceId, ToPoulePlaceId ) ( SELECT QualifiyingPoulePlaceId, Id FROM PoulePlaces WHERE QualifiyingPoulePlaceId IS NOT NULL )</sql>
                <sql>insert into QualifyRules( FromRoundId, ToRoundId, ConfigNr, TmpFromPoulePlaceId, TmpToPoulePlaceId ) ( SELECT FP.RoundId, TP.RoundId, 0, PP.QualifiyingPoulePlaceId, PP.Id FROM PoulePlaces PP JOIN PoulePlaces FPP ON FPP.Id = PP.QualifiyingPoulePlaceId JOIN Poules FP ON FP.Id = FPP.PouleId JOIN Poules TP ON TP.Id = PP.PouleId WHERE PP.QualifiyingPoulePlaceId IS NOT NULL )</sql>
                <sql>update PoulePlaceQualifyRules SET QualifyRuleId =
                    ( 	SELECT 	Id
                    FROM 	QualifyRules
                    WHERE 	TmpFromPoulePlaceId = PoulePlaceQualifyRules.FromPoulePlaceId
                    AND 	TmpToPoulePlaceId = PoulePlaceQualifyRules.ToPoulePlaceId
                    )
                </sql>
            </queries>
        </Addition>
