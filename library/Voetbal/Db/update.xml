          <queries>
              <sql>CREATE TEMPORARY TABLE PoulesTmp ( Id int NOT NULL, Number tinyint NOT NULL )</sql>
              <sql>INSERT INTO PoulesTmp( Id, Number) ( SELECT Id, ( SELECT COUNT(*) FROM Poules PSUB WHERE PSUB.RoundId = P.RoundId AND PSUB.Id &lt; P.Id ) FROM Poules P WHERE P.Number IS NULL )</sql>
              <sql>UPDATE Poules P JOIN PoulesTmp PTMP ON P.Id = PTMP.Id SET P.Number = PTMP.Number</sql>
              <sql>DROP TABLE PoulesTmp</sql>
